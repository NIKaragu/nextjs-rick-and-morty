{
  "plugins": ["prettier-plugin-tailwindcss"],
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "useTabs": true,
  "bracketSpacing": true,
  "tsxBracketSameLine": false,
  "arrowParens": "avoid",
  "printWidth": 80,
  "endOfLine": "lf",
  "trailingComma": "all",
  "jsxSingleQuote": false,
  "bracketSameLine": false
}
